<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.funsonli.bootan.module.my.mapper.StudentMapper">

    <update id="updateSortOrder">
         update tbl_student set sort_order = sort_order - 5 where id = #{id} and sort_order = #{sortOrder}
    </update>

</mapper>
